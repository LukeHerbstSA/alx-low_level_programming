#include <stdio.h>
#include "main.h"
/**
	* main - Entry point
	* Description:
	* Parameter:
	* Return: Always 0 (Success)
	*/
int print_sign(int n);
int main(void)
{
	int print_sign();
	putchar('\n');
}
/**
	* print_sign - checks sign
	* Description:
	* Parameter:
	* Return: Always 0 (Success)
	*/
int print_sign(int n)
{
	if (n > 0)
	{
		putchar('+');
		return (1);
	}
	else if (n == 0)
	{
		putchar('0');
		return(0);
	}
	else
	{
		putchar('-');
		return(-1);
	}
}
