#include "main.h"

/**
	* print_array - void function
	* Description: prints n elements of an array
	* @a: int pointer to array
	* @n: int
	*/
void print_array(int *a, int n)
{
	int i;

	for (i = 0; i < n; i++)
		if (i == (n - 1))
			printf("%d", a[i]);
		else
			printf("%d, ", a[i]);
	putchar('\n');
}
